{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "moduleResolution": "Node",
    "checkJs": false,
    "declaration": true,
    "sourceMap": true,
    "allowJs": true,
    "noEmitOnError": true,
    "lib": ["es2017", "dom"],
    "strict": false,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "useUnknownInCatchVariables": false,
    "importHelpers": true,
  },
}
